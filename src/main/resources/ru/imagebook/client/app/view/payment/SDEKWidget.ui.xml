<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:HTMLPanel ui:field="sdekWidgetPanel" visible="false">
		<div id="sdekWidget" class="sdek-widget"/>
		<g:Label ui:field="sdekWidgetDisabledLabel" styleName="error" text="CDEK widget не сконфигурирован" visible="false"/>

		<g:VerticalPanel ui:field="costAndTimeFieldPanel" visible="false" styleName="sdek-cost-and-time">
			<g:HorizontalPanel ui:field="courierCompanyPanel" verticalAlignment="top" visible="false">
				<g:Label text="Доставка курьером.&#160;"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel ui:field="pickupCompanyPanel" verticalAlignment="top" visible="false">
				<g:Label text="Самовывоз.&#160;"/>
				<g:HTMLPanel>
					<span ui:field="pickUpCompanyField"/>
					<br/>
				</g:HTMLPanel>
			</g:HorizontalPanel>
			<g:HorizontalPanel verticalAlignment="top">
				<g:Label text="Стоимость доставки:&#160;"/>
				<g:HTMLPanel>
					<span ui:field="costField"/>
					<br/>
				</g:HTMLPanel>
			</g:HorizontalPanel>
			<g:HorizontalPanel verticalAlignment="top">
				<g:Label text="Срок:&#160;"/>
				<g:HTMLPanel>
					<span ui:field="timeField"/>
				</g:HTMLPanel>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>
