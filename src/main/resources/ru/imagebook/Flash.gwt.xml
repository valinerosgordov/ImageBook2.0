<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE module SYSTEM "gwt-module.dtd">

<module rename-to="gwt.flash">
	<source path="client">
		<include name="flash/**" />
		<include name="common/**" />
	</source>
	<source path="shared" />

	<inherits name="com.google.gwt.user.User" />

	<inherits name='com.google.gwt.user.theme.standard.Standard' />

	<inherits name="ru.minogin.core.Core" />
	<inherits name="ru.saasengine.SaasEngine" />

	<extend-property name="locale" values="ru" />

	<entry-point class="ru.imagebook.client.flash.FlashEntryPoint" />
</module>